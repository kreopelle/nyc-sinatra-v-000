<h1><%= @figure.name %></h1>

<h2>Titles</h2>

<ul>
  <% @figure.titles.each do |title| %>
    <li><%= title.name %></li><br>
  <% end %>
</ul>

<h2>Landmarks</h2>

<br></br>

<ul>
  <% @figure.landmarks.each do |landmark| %>
    <li>Name: <%= landmark.name %> Year Completed:<%= landmark.year_completed %></li><br>
  <%end%>
</ul>
